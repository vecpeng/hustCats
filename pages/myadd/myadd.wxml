<!--pages/myadd/myadd.wxml-->
<view class="middle" style="display:{{cats=='[]'?'none':'block'}}">去添加你的第一只猫猫吧(^-^)</view>
<view style="width:94vw;margin:auto">
<view class="cats" wx:for="{{cats}}" wx:item="item" wx:key="*this">
<image class="catsImage" src="{{item.indexImg}}" mode="aspectFill"></image>
<text class="catsName">{{item.name}}</text>
</view>
</view>
