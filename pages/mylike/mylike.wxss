/* pages/mylike/mylike.wxss */
page {
  background-color: #e0e0e0;
}

.cats {
  width: 43vw;
  height: 45vw;
  margin: 4vw 2vw;
  display: inline-block;
  border-radius: 4%;
  background-color: #ffffff;
  box-shadow: #666666 3px 3px 2px;
}

.catsImage {
  border-radius: 4% 4% 0 0;
  width: 43vw;
  height: 36vw;
}

.catsName {
  position: relative;
  left: 4vw;
  font-weight: bold;
}